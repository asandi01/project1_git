@model IEnumerable<Project1.Models.AlertModel>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/shared/_LayoutPage.cshtml";
}
<!-- Page Heading -->
<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">
            Payment Record
            <small>By Expense Category</small>
        </h1>
        <ol class="breadcrumb">
            <li>
                <i class="fa fa-dashboard"></i>  <a href="/">Dashboard</a>
            </li>
            <li class="active">
                <i class="fa fa-file"></i> Income Record By Expense Category
            </li>
        </ol>
    </div>
</div>
<!-- /.row -->

<div class="row">
    <div class="col-xl-12">

        <table class="table">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.detail)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.amount)
                </th>
            </tr>

            @foreach (var item in Model) {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.detail)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.amount)
                    </td>
                </tr>
            }

        </table>

    </div>
</div>

